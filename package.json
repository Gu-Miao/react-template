{
  "name": "react-template",
  "version": "1.0.0",
  "description": "react template based on CRA",
  "keywords": [
    "react",
    "template",
    "antd",
    "less",
    "decorator"
  ],
  "repository": {
    "url": "git+https://github.com/Gu-Miao/react-template.git",
    "type": "git"
  },
  "homepage": "https://github.com/Gu-Miao/react-template",
  "config": {
    "port": "4000",
    "public": "."
  },
  "scripts": {
    "start": "cross-env PORT=$npm_package_config_port PUBLIC_URL=$npm_package_config_public craco start",
    "build": "cross-env PUBLIC_URL=$npm_package_config_public GENERATE_SOURCEMAP=false craco build",
    "test": "craco test",
    "format": "prettier --write \"src/**/*.{js,jsx,css,less}\"",
    "lint": "eslint --ext js src --max-warnings 0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm run lint"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "antd": "^4.8.4",
    "axios": "^0.19.2",
    "classnames": "^2.2.6",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-cache-route": "^1.10.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^3.4.3"
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.10.4",
    "@craco/craco": "^5.6.4",
    "babel-plugin-import": "^1.13.0",
    "craco-less": "^1.17.0",
    "cross-env": "^7.0.2",
    "http-proxy-middleware": "^1.0.4",
    "husky": "^4.3.0",
    "prettier": "^2.0.5"
  }
}
